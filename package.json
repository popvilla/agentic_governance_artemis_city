  "name": "artemis-city",
  "version": "1.0.0",
  "type": "module",
  "description": "Architectural framework for agentic reasoning with transparent, accountable action across distributed intelligence systems.",
  "main": "dist/index.js",
  "scripts": {
    "install:dev": "npm install",
    "install:prod": "npm install --omit=dev",
    "install:ci": "npm ci",
    "install:ci:prod": "npm ci --omit=dev",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node app/src/index.ts",
    "dev:watch": "nodemon --watch app/src --ext ts --exec 'npm run build && npm start'",
    "serve": "node dist/index.js",
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "clean": "rm -rf dist node_modules/.cache",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "rebuild": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "test": "pytest",
    "test:run": "pytest tests/",
    "test:watch": "pytest-watch",
    "test:coverage": "pytest --cov=app --cov-report=html --cov-report=term",
    "test:integration": "pytest tests/integration/ -m integration",
    "test:all": "npm run test:run && npm run typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:py": "flake8 app tests && mypy app tests",
    "lint:py:fix": "black app tests && isort app tests",
    "format": "prettier --write \"app/**/*.{ts,json}\" \"*.{json,yml,yaml,md}\"",
    "format:check": "prettier --check \"app/**/*.{ts,json}\" \"*.{json,yml,yaml,md}\"",
    "format:py": "black app tests && isort app tests",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "validate:all": "npm run validate && npm run lint:py && npm run test:run",
    "docker:build": "docker build -t artemis-city .",
    "docker:run": "docker run -p 3000:3000 artemis-city",
    "docker:compose": "docker-compose up",
    "docker:compose:build": "docker-compose up --build",
    "preversion": "npm run validate",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags",
    "prepare": "husky"
  },
  "keywords": [
    "agents",
    "ai",
    "agentic",
    "governance",
    "trust",
    "memory",
    "distributed-systems",
    "artificial-intelligence",
    "mcp",
    "obsidian",
    "llm"
  ],
  "author": "Prinston Palmer <prinstonpalmer879@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.11",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{json,yml,yaml}": "prettier --write"
  }
}
